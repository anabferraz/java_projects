import java.util.Scanner;

public class Exercicio10{
    public static void main (String[]args){
        int num, cont;
        cont = 1;
        Scanner scan = new Scanner(System.in);
        System.out.println("Digite um n√∫mero: ");
        num = scan.nextInt();
        while (cont<=10){
            System.out.println(num+ " x "+cont+" = "+ num*cont);
            cont++;
        }
    }
}
